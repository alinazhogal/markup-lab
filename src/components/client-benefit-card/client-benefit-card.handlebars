<div class="benefit-card">
  <div class="circle">
  <img src="{{benefitIcon}}" alt="{{{iconAlt}}}">
  </div>
  <h5>{{headerName}}</h5>
  <div class="benefit-info">
    {{#benefitsList}}
    <ul>
      <li>- {{this.name}}</li>
    </ul>
    {{/benefitsList}}
    <a href="{{learnLink}}">Learn more</a>
  </div>
</div>